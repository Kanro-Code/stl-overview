{
  "name": "stl-overview",
  "version": "0.0.6",
  "description": "Stl Overview does just that, creates a image from a bunch of stls to give a great overview of what models you have.",
  "main": "src/electron/index.js",
  "scripts": {
    "test": "jest",
    "start": "electron src/electron/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Kanro-Code",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "jimp": "^0.16.1",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "jest": "^26.4.2",
    "standard": "^14.3.4"
  },
  "build": {
    "appId": "com.electron.stl-overview",
    "icon": "dev/simple-icon.ico",
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": [
        {
          "target": "dmg",
          "arch": "x64"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": "x64"
        },
        {
          "target": "nsis-web",
          "arch": "x64"
        }
      ]
    }
  },
  "standard": {
    "ignore": [
      "*test.js"
    ]
  }
}
